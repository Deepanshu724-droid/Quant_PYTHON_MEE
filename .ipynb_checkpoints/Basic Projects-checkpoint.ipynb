    "print(\"Welcome to dice simulation\")\n",
    "DICE_FACE = [1,2,3,4,5,6]\n",
    "DICE_no=float(input(\"how many times you want to roll a dice ? \"))\n",
    "import random \n",
    "i= 1 \n",
    "while i<= DICE_no:\n",
    "    res = random.choice(DICE_FACE)\n",
    "    print(res)\n",
    "    i+=1 \n",
    "    \n",
    "    "
